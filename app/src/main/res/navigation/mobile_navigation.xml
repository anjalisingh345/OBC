<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.e.obc.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.e.obc.ui.dashboard.DashboardFragment"
        android:label="Search"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.e.obc.ui.notifications.NotificationsFragment"
        android:label="Booking"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_categories"
        android:name="com.e.obc.CategoriesFragment"
        android:label="Categories"
        tools:layout="@layout/activity_categories_fragment" />

</navigation>